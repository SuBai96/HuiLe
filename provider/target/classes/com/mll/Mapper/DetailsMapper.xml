<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mll.Mapper.DetailsMapper">
    <!--//查询指定商品详情-->
    <select id="detailsAll" resultType="Details">
      SELECT
        *
        FROM
        `MLL_PRODUCT`
        WHERE mp_id=#{id}
    </select>
</mapper>